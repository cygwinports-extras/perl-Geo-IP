  /usr/lib/perl5/vendor_perl/5.8/cygwin/Geo/IP/Record.pod
  /usr/lib/perl5/vendor_perl/5.8/cygwin/Geo/Mirror.pm
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/Geo/IP/.packlist
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/Geo/IP/IP.bs
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/Geo/IP/IP.dll
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/Geo/IP/libIP.dll.a
  /usr/share/doc/Cygwin/perl-Geo-IP-1.30.README
  /usr/share/doc/perl-Geo-IP-1.30/Changes
  /usr/share/doc/perl-Geo-IP-1.30/INSTALL
